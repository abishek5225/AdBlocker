{
    "manifest_version": 2,
    "name": "LinkedIn AdBlocker",
    "description": "Blocking ads",
    "version": "0.0.1",
    "author": "me",
    "browser_action":{
        "default_title": "LinkedIn AdBlocker"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://www.Linkedin.com"
    ],
    "background":{
        "scripts": [
            "extension.js"
        ]
    }

}